    //定义打印长方形的方法
    public static void printRectangle(int height, int weight) {
        for (int i = 0; i < weight; i++) {
            for (int j = 0; j < height; j++) {
                System.out.print("*");
            }
            System.out.print("\n");
        }
        System.out.print("\n");
    }

    //周长
    public static double perimeter(double height, double weight) {
        double perimeter;
        perimeter = height * 2 + weight * 2;
        return perimeter;
    }

    //面积
    public static double area(double height, double weight) {
        double area;
        area = height * weight;
        return area;
    }

    public static void ziJia(int x, int y) {
        x++;
        y++;
    }

    public static double distance(int x, int y, int a, int b) {
        double distance;
        distance = Math.sqrt((a - x) * (a - x) + (b - y) * (b - y));
        return distance;
    }

    public static int sum(int a, int b) {
        int sum = a + b;
        return sum;
    }

    public static int sum(int a, int b, int c) {
        int sum = a + b;
        return sum;
    }

    public static float sum(float a, float b) {
        float sum = a + b;
        return sum;
    }

    public static int getmax(int[] a) {
        int max = a[0];
        for (int i = 1; i < a.length; i++) {
            if (a[i] > max) {
                max = a[i];
            }
        }
        return max;
    }
    }

